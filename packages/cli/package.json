{
  "name": "forrest",
  "version": "2.7.5",
  "description": "CLI Utility for ForrestJS",
  "main": "index.js",
  "bin": {
    "forrest": "index.js"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm -f yarn.lock && rm -f package-lock.json",
    "test": "jest",
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
    "watch:to": "babel -w src --out-dir \"${TARGET}/node_modules/${npm_package_name}\" --ignore **/__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forrestjs/forrestjs.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cra",
    "create-react-app",
    "ssr",
    "cli",
    "starter"
  ],
  "author": "Marco Pegoraro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forrestjs/forrestjs/issues"
  },
  "homepage": "https://github.com/forrestjs/forrestjs/tree/master/packages/cli#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@forrestjs/babel-preset-package": "^2.7.3",
    "cross-env": "^5.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.4",
    "clipboardy": "^2.0.0",
    "fs-extra": "^8.0.0",
    "git-clone": "^0.1.0",
    "update-notifier": "^3.0.0",
    "yargs": "^13.2.4"
  }
}
